{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node@20"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@20",
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
