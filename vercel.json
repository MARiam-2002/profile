{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node@20"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/index.js"
    }
  ],
  "functions": {
    "backend/api/index.js": {
      "runtime": "@vercel/node@20"
    }
  }
}
